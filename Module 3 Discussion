using System;

public class Bunny
{
  public string Name;
  public bool LikesCarrots;
  public bool LikesHumans;

  public Bunny 
(string name,
bool likesCarrots = false, 
bool likesHumans = false)
  {
    Name = name;
    LikesCarrots = likesCarrots;
    LikesHumans = likesHumans; 
  }
}

public class Bunny2
{
    public static void Main ()
    {
      Bunny newbunny = new bunny ("Hopper");
       {
          LikesCarrots = true;
          LikesHumans = true;

  }
}
Console.WriteLine("{newbunny.Name} -LikesCarrots:{newbunny.LikesCarrots}")
Console.WriteLine("{newbunny.Name} -LikesHumans:{newbunny.LikesHuman}")
}
